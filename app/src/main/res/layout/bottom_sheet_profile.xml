<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rootView"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:overScrollMode="never">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <include
                android:id="@+id/includePersonalInformationView"
                layout="@layout/item_row_drawable_text_view_bottom_divider_sample_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <include
                android:id="@+id/myHairLossJourneyView"
                layout="@layout/item_row_drawable_text_view_bottom_divider_sample_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <include
                android:id="@+id/includeHairLossJourneyView"
                layout="@layout/item_row_drawable_text_view_bottom_divider_sample_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                android:id="@+id/includeRegionalSettingsView"
                layout="@layout/item_row_drawable_text_view_bottom_divider_sample_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                android:id="@+id/includeNotificationsView"
                layout="@layout/item_row_drawable_text_view_bottom_divider_sample_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                android:id="@+id/includeGetHelpView"
                layout="@layout/item_row_drawable_text_view_bottom_divider_sample_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                android:id="@+id/includeGiveFeedbackView"
                layout="@layout/item_row_drawable_text_view_bottom_divider_sample_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                android:id="@+id/includeTermsOfServiceView"
                layout="@layout/item_row_drawable_text_view_bottom_divider_sample_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                android:id="@+id/includePrivacyPolicyView"
                layout="@layout/item_row_drawable_text_view_bottom_divider_sample_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <include
                android:id="@+id/includeLogOutView"
                layout="@layout/item_row_drawable_text_view_bottom_divider_sample_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</FrameLayout>